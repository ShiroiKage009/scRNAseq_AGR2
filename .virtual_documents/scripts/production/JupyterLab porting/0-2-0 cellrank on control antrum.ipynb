adata = ad

from cellrank.kernels import PseudotimeKernel

pk = PseudotimeKernel(adata, time_key="palantir_LGR5")


pk.compute_transition_matrix()


pk.plot_random_walks(
    n_sims = 300,
    linewidth = 0.5,
    start_ixs = {"leiden": "LGR5 stem"},
    basis = "umap",
    size = 25,
)


